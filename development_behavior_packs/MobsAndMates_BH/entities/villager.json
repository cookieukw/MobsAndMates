{
  "format_version": "1.8.0",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:villager_v2",
      "is_spawnable": true,
      "is_summonable": true
    },
    "components": {
      "minecraft:type_family": { "family": ["player", "villager"] },
      "minecraft:health": { "value": 40, "max": 40 },
      "minecraft:movement": { "value": 0.35 },
      "minecraft:movement.basic": {},
      "minecraft:attack": { "damage": 6 },
      "minecraft:inventory": {
        "container_type": "inventory",
        "inventory_size": 24,
        "private": false
      },
      "minecraft:equippable": {
        "slots": [
          { "slot": 0, "accepted_items": ["minecraft:iron_sword"] },
          { "slot": 2, "accepted_items": ["minecraft:iron_helmet"] },
          { "slot": 3, "accepted_items": ["minecraft:iron_chestplate"] },
          { "slot": 4, "accepted_items": ["minecraft:iron_leggings"] },
          { "slot": 5, "accepted_items": ["minecraft:iron_boots"] }
        ]
      },
      "minecraft:admire_item": {},
      "minecraft:loot": "loot_tables/entities/villager.json",

      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "can_swim": true,
        "avoid_water": false,
        "avoid_damage_blocks": true,
        "can_pass_doors": true,
        "can_open_doors": true
      },
      "minecraft:behavior.melee_attack": {
        "priority": 1,
        "attack_once": false,
        "speed_multiplier": 1.25,
        "reach_multiplier": 1.5,
        "min_path_time": 0.2,
        "max_path_time": 0.55,
        "path_inner_boundary": 16,
        "path_outer_boundary": 32,
        "inner_boundary_time_increase": 0.25,
        "outer_boundary_time_increase": 0.5,
        "path_fail_time_increase": 0.75,
        "require_complete_path": false,
        "melee_fov": 90
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1,
        "alert_same_type": true,
        "hurt_owner": true,
        "max_dist": 16,
        "cooldown": 0.0
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "monster"
            }
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "undead"
            }
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "zombie"
            }
          }
        ],
        "must_see": true,
        "max_dist": 20,
        "attack_owner": false,
        "scan_interval": 10,
        "reselect_targets": true
      },
      "minecraft:behavior.avoid_mob_type": {
        "priority": 3,
        "entity_types": [],
        "max_dist": 10.0,
        "walk_speed_multiplier": 1.3,
        "sprint_speed_multiplier": 1.6,
        "avoid_target_xz": 16,
        "avoid_target_y": 7,
        "ignore_visibility": false,
        "max_flee": 10.0
      },

       "minecraft:equip_item": {
        "can_wear_armor": true
      },
      "minecraft:behavior.equip_item": { "priority": 4 },
      "minecraft:behavior.open_door": {
        "priority": 5,
        "close_door_after": true
      },
      "minecraft:behavior.flee_sun": { "priority": 5 },
      "minecraft:behavior.find_cover": { "priority": 5 },
      "minecraft:behavior.move_to_block": { "priority": 5 },
   
      "minecraft:behavior.fertilize_farm_block": { "priority": 5 },
      "minecraft:behavior.work": { "priority": 6 },
      "minecraft:behavior.work_composter": { "priority": 6 },
      "minecraft:behavior.celebrate": { "priority": 7 },
      "minecraft:behavior.trade_with_player": { "priority": 8 },
      "minecraft:behavior.pickup_items": {
        "priority": 9,
        "goal_radius": 2,
        "speed_multiplier": 0.5,
        "can_pickup_to_hand_or_equipment": true,
        "max_dist": 3,
        "excluded_items": []
      },
      "minecraft:behavior.share_items": {
        "priority": 10,
        "share_with_family": ["villager", "villager_v2", "player"],
        "item_filters": [
          { "item": "minecraft:bread" },
          { "item": "minecraft:apple" },
          { "item": "minecraft:emerald" },
          { "item": "minecraft:iron_sword" },
          { "item": "minecraft:shield" }
        ]
      },
      "minecraft:behavior.take_flower": {
        "priority": 11,
        "filters": { "all_of": [{ "test": "is_daytime", "value": true }] }
      },
      "minecraft:behavior.admire_item": {
        "priority": 12,
        "admire_item_sound": "admire",
        "sound_interval": { "min": 2, "max": 5 }
      },
      "minecraft:behavior.tempt": {
        "priority": 13,
        "items": ["minecraft:emerald", "minecraft:bread", "minecraft:apple"],
        "speed_multiplier": 1.2
      },
      "minecraft:behavior.move_indoors": { "priority": 14 },
      "minecraft:behavior.move_outdoors": { "priority": 14 },
      "minecraft:behavior.sleep": { "priority": 15 },
 
      "minecraft:behavior.look_at_player": {
        "priority": 17,
        "look_distance": 8
      },
      "minecraft:behavior.look_at_entity": {
        "priority": 17,
        "look_distance": 6
      },
      "minecraft:behavior.random_look_around": { "priority": 18 },
      "minecraft:behavior.follow_mob": {
        "priority": 19,
        "speed_multiplier": 1.1,
        "target_family": ["player"]
      },
      "minecraft:behavior.random_stroll": { "priority": 20 },
      "minecraft:behavior.offer_flower": { "priority": 21 },
      "minecraft:behavior.play": { "priority": 21 },
      "minecraft:behavior.stay_while_sitting": { "priority": 21 },
      "minecraft:behavior.random_sitting": { "priority": 21 },
      "minecraft:behavior.random_look_around_and_sit": { "priority": 21 },
      "minecraft:behavior.move_to_liquid": { "priority": 22 },
      "minecraft:behavior.move_to_land": { "priority": 22 },
      "minecraft:behavior.inspect_bookshelf": { "priority": 22 },
      "minecraft:behavior.send_event": { "priority": 22 },

      "minecraft:shareables": {
        "singular_pickup": true,
        "items": [
          {
            "item": "minecraft:golden_sword",
            "priority": 2,
            "admire": true,
            "pickup_limit": 1,
            "stored_in_inventory": true
          },
          {
            "item": "minecraft:porkchop",
            "consume_item": true,
            "priority": 3,
            "max_amount": 64
          },
          {
            "item": "minecraft:gold_ingot",
            "priority": 1,
            "pickup_limit": 1,
            "admire": true,
            "barter": true
          }
        ]
      },

      "minecraft:behavior.move_to_village": { "priority": 6 },
      "minecraft:nameable": {
        "always_show": true,
        "allow_name_tag_renaming": false
      }
    }
  }
}
