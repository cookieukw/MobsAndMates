{
  "format_version": "1.21.50",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:villager_v2",
      "is_spawnable": true,
      "is_summonable": true
    },
    "component_groups": {
      "minecraft:default_targeting": {
        "minecraft:behavior.nearest_attackable_target": {
          "priority": 2,
          "must_see": true,
          "within_radius": 12.0,
          "must_see_forget_duration": 40.0,
          "entity_types": [
            {
              "filters": {
                "any_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "illager"
                  },
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "monster"
                  },
                  { "test": "is_family", "subject": "other", "value": "undead" }
                ]
              },
              "max_dist": 12
            },
            {
              "filters": {
                "all_of": [
                  {
                    "test": "is_family",
                    "subject": "other",
                    "value": "creeper"
                  },
                  {
                    "test": "has_component",
                    "subject": "other",
                    "operator": "!=",
                    "value": "minecraft:creeper"
                  }
                ]
              },
              "max_dist": 12
            }
          ]
        }
      }
    },
    "components": {
      "minecraft:type_family": {
        "family": ["villager", "mob"]
      },
      "minecraft:health": {
        "value": 20,
        "max": 20
      },
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:attack": {
        "damage": 8
      },

      "minecraft:follow_range": {
        "value": 64
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "can_pass_doors": true,
        "can_open_doors": true,
        "can_swim": true,
        "avoid_water": false
      },

      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:nameable": {},
      "minecraft:persistent": {},
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {},
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },

      "minecraft:inventory": {
        "container_type": "inventory",
        "inventory_size": 24,
        "private": false
      },
      "minecraft:equip_item": {},
      "minecraft:equippable": {
        "slots": [
          {
            "slot": 0,
            "item": "slot.weapon.mainhand",
            "accepted_items": [
              "minecraft:iron_sword",
              "minecraft:diamond_sword",
              "minecraft:netherite_sword",
              "minecraft:iron_axe",
              "minecraft:diamond_axe",
              "minecraft:netherite_axe",
              "minecraft:bow",
              "minecraft:crossbow"
            ]
          },
          {
            "slot": 1,
            "item": "slot.weapon.offhand",
            "accepted_items": ["minecraft:shield"]
          },
          {
            "slot": 2,
            "item": "slot.armor.head",
            "accepted_items": [
              "minecraft:leather_helmet",
              "minecraft:chainmail_helmet",
              "minecraft:iron_helmet",
              "minecraft:diamond_helmet",
              "minecraft:netherite_helmet",
              "minecraft:turtle_helmet"
