            "max_dist": 25
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "illager"
            },

            "max_dist": 25
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "zombie"
            },

            "ma,x_dist": 25
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "undead"
            },

            "max_dist": 25
