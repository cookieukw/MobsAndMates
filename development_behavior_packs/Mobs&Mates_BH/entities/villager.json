{
  "format_version": "1.21.20",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:villager",
      "is_spawnable": true,
      "is_summonable": true,
      "runtime_identifier": "minecraft:villager_v4"
    },
    "components": {
      "minecraft:type_family": {
        "family": ["villager", "playerlike", "humanoid"]
      },
      "minecraft:health": { "value": 40, "max": 40 },
      "minecraft:movement": { "value": 0.35 },
      "minecraft:movement.basic": {},
      "minecraft:attack": { "damage": 6 },
      "minecraft:inventory": {
        "container_type": "inventory",
        "inventory_size": 24,
        "private": false
      },

      "minecraft:equippable": {
        "slots": [
          {
            "slot": 0,

            "accepted_items": [
              "minecraft:iron_sword",
              "minecraft:diamond_sword",
              "minecraft:netherite_sword"
            ]
          },
          {
            "slot": 1,

            "accepted_items": ["minecraft:shield"]
          },
          {
            "slot": 2,

            "accepted_items": [
              "minecraft:leather_helmet",
              "minecraft:iron_helmet",
              "minecraft:diamond_helmet",
              "minecraft:netherite_helmet"
            ]
          },
          {
            "slot": 3,

            "accepted_items": [
              "minecraft:leather_chestplate",
              "minecraft:iron_chestplate",
              "minecraft:diamond_chestplate",
              "minecraft:netherite_chestplate"
            ]
          },
          {
            "slot": 4,

            "accepted_items": [
              "minecraft:leather_leggings",
              "minecraft:iron_leggings",
              "minecraft:diamond_leggings",
              "minecraft:netherite_leggings"
            ]
          },
          {
            "slot": 5,

            "accepted_items": [
              "minecraft:leather_boots",
              "minecraft:iron_boots",
              "minecraft:diamond_boots",
              "minecraft:netherite_boots"
            ]
          }
        ]
      },
      "minecraft:admire_item": {},
      "minecraft:loot": "loot_tables/entities/villager.json",

      "minecraft:behavior.random_stroll": {
        "priority": 18,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.equip_item": {
        "priority": 3
      },
      "minecraft:behavior.take_flower": {
        "priority": 9,
        "filters": {
          "all_of": [
            {
              "test": "is_daytime",
              "value": true
            }
          ]
        }
      },
      "minecraft:equip_item": {
        "can_wear_armor": true
      },
      "minecraft:behavior.follow_mob": {
        "priority": 17,
        "speed_multiplier": 1.1,
        "target_family": ["playerlike", "villager"]
      },
      "minecraft:behavior.random_look_around": { "priority": 16 },
      "minecraft:behavior.look_at_player": {
        "priority": 15,
        "look_distance": 8
      },
      "minecraft:behavior.look_at_entity": {
        "priority": 15,
        "look_distance": 6
      },

      "minecraft:behavior.open_door": {
        "priority": 6,
        "close_door_after": true
      },

      "minecraft:navigation.walk": {
        "avoid_water": false
      },
      "minecraft:navigation.fly": {
        "avoid_water": false
      },
      "minecraft:navigation.float": {
        "avoid_water": false
      },
      "minecraft:navigation.hover": {
        "avoid_water": false
      },
      "minecraft:navigation.generic": {
        "can_walk": true,
        "can_path_over_water": true,
        "can_swim": true,
        "can_fly": true,
        "can_float": true,
        "avoid_water": false,
        "avoid_damage_blocks": true,
        "avoid_portals": true,
        "avoid_sun": false,
        "jump_up_blocks": true,
        "sink_in_water": false,
        "hover": true
      },

      "minecraft:behavior.move_to_village": { "priority": 13 },
      "minecraft:dweller": {
        "dwelling_type": "village",
        "dweller_role": "defender",
        "update_interval_base": 60,
        "update_interval_variant": 40,
        "can_find_poi": false,
        "can_migrate": true,
        "first_founding_reward": 0
      },
      "minecraft:behavior.explore_outskirts": {
        "priority": 9,
        "next_xz": 5,
        "next_y": 3,
        "min_wait_time": 3,
        "max_wait_time": 10,
        "max_travel_time": 60,
        "speed_multiplier": 0.6,
        "explore_dist": 6,
        "min_perimeter": 1,
        "min_dist_from_target": 2.5,
        "timer_ratio": 2,
        "dist_from_boundary": [5, 0, 5]
      },
      "minecraft:behavior.move_indoors": { "priority": 13 },
      "minecraft:behavior.move_outdoors": { "priority": 13 },

      "minecraft:behavior.sleep": { "priority": 12 },

      "minecraft:behavior.trade_with_player": { "priority": 11 },

      "minecraft:behavior.pickup_items": {
        "can_pickup_any_item": true,
        "priority": 4,
        "max_dist": 20,
        "search_height": 10,
        "speed_multiplier": 1.5,
        "can_pickup_to_hand_or_equipment": true,
        "cooldown_after_being_attacked": 5
      },
      "minecraft:behavior.share_items": {
        "priority": 10,
        "share_with_family": ["villager", "player"],
        "item_filters": [
          { "item": "minecraft:bread" },
          { "item": "minecraft:apple" },
          { "item": "minecraft:emerald" },
          { "item": "minecraft:iron_sword" },
          { "item": "minecraft:shield" }
        ]
      },

      "minecraft:behavior.admire_item": {
        "priority": 9,
        "admire_item_sound": "admire",
        "sound_interval": { "min": 2, "max": 5 }
      },
      "minecraft:behavior.tempt": {
        "priority": 9,
        "items": ["minecraft:emerald", "minecraft:bread", "minecraft:apple"],
        "speed_multiplier": 1.2
      },

      "minecraft:behavior.avoid_mob_type": {
        "priority": 8,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "zombie"
            },
            "max_dist": 10,
            "walk_speed_multiplier": 1.3,
            "sprint_speed_multiplier": 1.6
          }
        ]
      },

      "minecraft:behavior.nearest_attackable_target": {
        "priority": 7,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "monster"
            },
            "must_see": true,
            "max_dist": 20
          }
        ]
      },

      "minecraft:behavior.melee_attack": { "priority": 6 },
      "minecraft:behavior.celebrate": { "priority": 6 },
      "minecraft:behavior.flee_sun": { "priority": 5 },
      "minecraft:behavior.find_cover": { "priority": 5 },

      "minecraft:behavior.move_to_block": { "priority": 5 },
      "minecraft:behavior.harvest_farm_block": { "priority": 5 },
      "minecraft:behavior.fertilize_farm_block": { "priority": 5 },

      "minecraft:behavior.work": { "priority": 4 },
      "minecraft:behavior.work_composter": { "priority": 4 },

      "minecraft:behavior.make_love": { "priority": 3 },
      "minecraft:behavior.receive_love": { "priority": 3 },
      "minecraft:behavior.offer_flower": { "priority": 3 },

      "minecraft:behavior.play": { "priority": 3 },
      "minecraft:behavior.stay_while_sitting": { "priority": 3 },
      "minecraft:behavior.random_sitting": { "priority": 3 },
      "minecraft:behavior.random_look_around_and_sit": { "priority": 3 },

      "minecraft:behavior.move_to_liquid": { "priority": 2 },
      "minecraft:behavior.move_to_land": { "priority": 2 },
      "minecraft:behavior.inspect_bookshelf": { "priority": 2 },
      "minecraft:behavior.send_event": { "priority": 2 }
    }
  }
}
